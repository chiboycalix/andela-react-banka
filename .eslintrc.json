{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb-base", "plugin:react/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "emcaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "no-undef": "off"
  }
}
